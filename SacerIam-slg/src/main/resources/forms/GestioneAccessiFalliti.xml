<?xml version="1.0" encoding="UTF-8"?>
<Form Description="GestioneAccessiFalliti" xmlns="http://www.spagoLite.eng.it/xmlbean/form" xmlns:xsi="http://www.w3.org/2001/XMLSchema" 
      xsi:schemaLocation="http://www.spagoLite.eng.it/xmlbean/form http://ibc3850.ente.regione.emr.it/xsd/latest/Form.xsd">
    
    <Fields Name="FiltriAccessiFalliti" Description="Ricerca degli accessi" >
        <Input Type="DATE" Description="Data accesso da" Name="dt_evento_da"/>
        <Input Type="INTEGER" Description="Orario" Name="ora_rif_da" Required="false"/>
        <Input Type="INTEGER" Description="Orario" Name="min_rif_da" Required="false"/>
        <Input Type="DATE" Description="Data accesso a" Name="dt_evento_a"/>    
        <Input Type="INTEGER" Description="Orario" Name="ora_rif_a" Required="false"/>
        <Input Type="INTEGER" Description="Orario" Name="min_rif_a" Required="false"/>
        <Input Type="STRING" Description="Username" Name="nm_userid"/>
        <ComboBox Type="STRING" Description="Tipo evento" Name="tipo_evento"/>
        <CheckBox Type="STRING" Description="Includi utenti automi" Name="incl_utenti_automi"/>
        <CheckBox Type="STRING" Description="Includi login OK" Name="incl_login_OK"/>
        <Input Type="STRING" Description="Nome" Name="nm_nome_user"/>
        <Input Type="STRING" Description="Cognome" Name="nm_cognome_user"/>
        <Input Type="STRING" Description="E-mail" Name="ds_email_user"/>
        <Input Type="STRING" Description="ID esterno" Name="cd_id_esterno"/>
        <Input Type="STRING" Description="Codice fiscale" Name="cd_fisc_user"/>
        <Button Name="ricercaAccessiFalliti" Description="Ricerca" Type="STRING"/>
    </Fields>
    
    <List Name="AccessiFallitiList" Description="Lista degli accessi">
        <Input Type="STRING" Description="Username" Name="nm_userid" />
        <Input Type="DATETIME" Description="Data evento" Name="dt_evento" />
        <Input Type="STRING" Description="Tipo evento" Name="tipo_evento" />
        <Input Type="STRING" Description="Descrizione evento" Name="ds_evento" />        
        <Input Type="STRING" Description="IP client" Name="cd_ind_ip_client" />
        <Input Type="STRING" Description="Indirizzo server" Name="cd_ind_server" />
        <Input Type="STRING" Description="Nome" Name="nm_nome_user" />
        <Input Type="STRING" Description="Cognome" Name="nm_cognome_user" />
        <Input Type="STRING" Description="Codice fiscale" Name="cd_fisc_user" />
        <Input Type="STRING" Description="E-mail" Name="ds_email_user" />
        <Input Type="STRING" Description="ID esterno" Name="cd_id_esterno" />
    </List>
     
</Form>
